<link rel="stylesheet" href="./sass/css/styleFeed.css" />

<div class="base-layout position-relative d-flex justify-content-center w-100 h-100">
   {{> nav}}

   <div class="content w-100 d-flex gap-0 h-auto pb-4 gap-3">
      <div class="col-3 rounded-bottom overflow-hidden pb-3 nav-container">
         <div class="left-nav">
            <ul
               class="nav h-100 d-flex flex-column align-items-center justify-content-start py-3 px-3 gap-3 ul-wrap">
               <li class="nav-item-left hover-icon active">
                  <a class="nav-link d-flex gap-3 align-items-center" aria-current="page" href="#"><i
                        class="fa-solid fa-house fa-fw"></i> <span>Home</span></a>
               </li>
               <li class="nav-item-left hover-icon">
                  <a class="nav-link d-flex gap-3 align-items-center" href="/profile">
                     <i class="fa-solid fa-user fa-fw"></i>
                     <span>Me</span></a>
               </li>
               <li class="nav-item-left hover-icon">
                  <a class="nav-link d-flex gap-3 align-items-center"><i
                        class="fa-solid fa-bell fa-fw"></i><span>Notifications</span></a>
               </li>
               <li class="nav-item-left hover-icon">
                  <a class="nav-link d-flex gap-3 align-items-center" href="/market"><i
                        class="fa-solid fa-store fa-fw"></i><span>Market</span></a>
               </li>
               <li class="nav-item-left hover-icon">
                  <a class="nav-link d-flex gap-3 align-items-center" href="/group"><i
                        class="fa-solid fa-users-line fa-fw"></i><span>Group</span></a>
               </li>

               <div class="small position-absolute " style="bottom:10px">Copyright &copy; Group 22</div>

            </ul>

         </div>

      </div>
      <div class="col-9 content-right">
         <div class="content-wrap overflow-hidden h-100 px-3   align-items-center">
            <div class="feed-posts d-flex flex-column gap-3 pb-3">

            </div>
         </div>
         <div class=" right-side">
            <div class="poll right-wrap mb-5">
               <div class="span6">
                  <h4>Book of the week</h4>
                  <hr class="my-3 mx-2" style="box-shadow: rgba(0, 0, 0, 0.15) 1.95px 1.95px 2.6px" />
                  <strong>Cơ sở trí tuệ nhân tạo</strong>
                  <div class="progress progress-danger active mb-3 mt-1">
                     <div class="progress-bar bg-warning" role="progressbar" style="width: 75%" aria-valuenow="75"
                        aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                  <strong>Phương pháp lập trình hướng đối tượng</strong>
                  <div class="progress progress-info active mb-3 mt-1">
                     <div class="progress-bar bg-info" role="progressbar" style="width: 50%" aria-valuenow="50"
                        aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                  <strong>Nhập môn lập trình</strong>
                  <div class="progress progress-striped active mb-3 mt-1">
                     <div class="progress-bar bg-success" role="progressbar" style="width: 25%" aria-valuenow="25"
                        aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                  <strong>Cơ sở dữ liệu</strong>
                  <div class="progress progress-success active mb-3 mt-1">
                     <div class="progress-bar bg-warning" role="progressbar" style="width: 75%" aria-valuenow="75"
                        aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                  <strong>Others</strong>
                  <div class="progress progress-warning active mb-3 mt-1">
                     <div class="progress-bar bg-danger" role="progressbar" style="width: 20%" aria-valuenow="20"
                        aria-valuemin="0" aria-valuemax="100"></div>
                  </div>
                  <button class="btn btn-large btn-success w-100 mt-3">Vote</button>
               </div>
            </div>

            <div class="right-wrap list-other-users mb-5">
               <div class="suggest-wrap mb-4">
                  <h4>Suggested people</h4>
                  <hr id="suggest-list" class="my-3 mx-2"
                     style="box-shadow: rgba(0, 0, 0, 0.15) 1.95px 1.95px 2.6px" />

               </div>
            </div>

            <div class="right-wrap spn-container pb-3">
               <h4 class="ps-3">Sponsors</h4>
               <hr id="sponsors-list" class="my-2 mx-3"
                  style="box-shadow: rgba(0, 0, 0, 0.15) 1.95px 1.95px 2.6px" />
            </div>
         </div>
      </div>
   </div>
</div>
</div>
</div>


<script type="module" src="./js/feed/feed.js"></script>